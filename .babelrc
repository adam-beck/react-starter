{
  "presets": [
    [
      "@babel/preset-env",
      {
        "targets": {
          "browsers": ["last 2 versions"]
        },
        "modules": false
      }
    ],
    "@babel/preset-react",
    "@babel/preset-typescript"
  ],
  "env": {
    "test": {
      "presets": [
        [
          "@babel/preset-env",
          {
            "targets": {
              "browsers": ["last 2 versions"]
            }
          }
        ],
        "@babel/preset-react",
        "@babel/preset-typescript"
      ]
    }
  },
  "plugins": ["react-hot-loader/babel", "transform-class-properties"]
}
